
number = [0,0,0,0,0]
x = 0
int(input(""))
while x < 5:
    number[x] =  int(input(f"Número {x + 1}: "))
    x += 1

while True:
    print(number)
    escolhido = int(input("Que posição você deseja imprimir? Digite '0' para SAIR: "))

    if escolhido == 0:
        break

    print(f"Você escolheu o número: {number[escolhido - 1]}")


